<%= form_for([@reward, @evaluation]) do |f| %>
  <% if @evaluation.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@evaluation.errors.count, "error") %> prohibited this evaluation from being saved:</h2>

      <ul>
      <% @evaluation.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :eval_number %><br />
    <%= f.number_field :eval_number %>
  </div>
  <div class="field">
    <%= f.label :level %><br />
    <%= f.number_field :level %>
  </div>
  <div class="field">
    <%= f.label :reward_id %><br />
    <%= f.number_field :reward_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
