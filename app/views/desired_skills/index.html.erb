<h1>Listing desired_skills</h1>

<table>
  <tr>
    <th>Language</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @desired_skills.each do |desired_skill| %>
  <tr>
    <td><%= desired_skill.language %></td>
    <td><%= link_to 'Show', desired_skill %></td>
    <td><%= link_to 'Edit', edit_desired_skill_path(desired_skill) %></td>
    <td><%= link_to 'Destroy', desired_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

