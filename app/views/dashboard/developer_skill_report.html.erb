<%= render 'layouts/header' %>

  <div id="tabbed_box_1" class="tabbed_box">
  
    <div id="navigation">
          <ul>
           <li><%= link_to 'All Requests', project_requests_path %></li>
           
           <li><%= link_to 'My Profile', employee_path(current_employee) %></li>
       
           <li><%= link_to "Requests I've Posted", _my_requests_path %></li>
           <li><%= link_to "Post a New Request", new_project_request_path %></li>
            <li><%= link_to "Dashboard", dashboard_path, :class => 'current' %></li>
          </ul>
        </div>

<div id="dashboard_navigation">

	<ul>
           <li><%= link_to 'request report', _request_report_path %></li>

           <li><%= link_to 'overall report', _overall_report_path %></li>
           <li><%= link_to 'time report', _time_report_path %></li>
           <li><%= link_to 'developer skill report', _developer_skill_report_path, :class => 'current_dash' %></li>
           <li><%= link_to 'average report', _average_report_path %></li>

            <li><%= link_to 'volunteer report', _volunteer_report_path %></li>
            <li><%= link_to 'shared report', _shared_report_path %></li>
            
    </ul>   
	</div>



<p class="dash_header_dev"> Developer Skill Interests Queries and Results</p>
<p class="dash_exit"></p>
<div class="dashreport_developer">
<table id="developer" class="tablesorter">
	<thead>
	<tr class="dash_title">
		<th>Total</th>
		<th>Language</th>
		<th>Chicago</th>
		<th>Boston</th>
		<th>Mumbai</th>
		<th>San Francisco</th>
		<th>London</th>
		<th>Houston</th>
	
	</tr>
</thead>
<tbody>
	<% Dashboard.new.developer_skill_report.each do |report| %>
	<tr>
		<td><%= report["TOTAL"] %></td>
		<td><%= report["language"] %></td>
		<td><%= report["Chicago"] %></td>
		<td><%= report["Boston"] %></td>
		<td><%= report["Mumbai"] %></td>
		<td><%= report["San_Francisco"] %></td>
		<td><%= report["London"] %></td>
		<td><%= report["Houston"] %></td>
	</tr>
	<% end %>
</tbody>
</table>
</div>	
<script type="text/javascript">
  $(document).ready(function()    
    {        
         $("#developer").tablesorter();   
    } 
); 
 </script>	