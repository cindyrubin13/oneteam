<p class="dash_header_share"> Developers Queries and Results for the First Three Months</p>

<div class="dashreport_shared1">
 <table id="threemonth" class="tablesorter">
	
	<thead>
	<tr class="dash_title">
		<th>Location</th>
		<th>Chicago</th>
		<th>San Francisco</th>
		<th>Houston</th>
		<th>London</th>
		<th>Mumbai</th>
		<th>Boston</th>
	</tr>

	</thead>
	<tbody>

	<% Dashboard.new.three_month_report.each do |report| %>
	<tr> 
		<% report.each do |key, value| %>
		  <td><%= value %></td>
	
		<% end %>
	</tr>
	<% end %>
</tbody>
</table>
</div>

